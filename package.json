    "electron-store": "^5.2.0",
    "electron-updater": "^4.6.1",
    "fs-extra": "^10.0.0",
    "http-proxy-agent": "^5.0.0",
    "https-proxy-agent": "^5.0.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "lodash": "^4.17.21",
    "mime": "^2.5.2",
    "mime-types": "^2.1.31",
    "mitt": "^2.1.0",
    "node-pty": "^0.10.1",
    "plantuml-pipe": "^1.3.6",
    "request": "^2.88.2",
    "safe-buffer": "^5.2.1",
    "socket.io": "^2.4.0",
    "socks-proxy-agent": "^6.1.1",
    "transliteration": "^2.2.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
